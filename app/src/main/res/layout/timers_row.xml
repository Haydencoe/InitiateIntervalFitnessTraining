<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <android.support.v7.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="341dp"
        android:layout_height="177dp"
        android:layout_gravity="top"
        android:layout_margin="10dp"
        android:background="#AFCBFF"
        card_view:cardBackgroundColor="#AFCBFF"
        card_view:cardCornerRadius="15dp"
        card_view:cardElevation="10dp"
        card_view:contentPadding="10dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="406dp"
            android:background="#AFCBFF">


            <ImageButton
                android:id="@+id/menuButton2"
                android:layout_width="36dp"
                android:layout_height="35dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:background="#AFCBFF"
                android:backgroundTint="#AFCBFF"
                android:onClick="showMenu"
                android:tint="@android:color/white"
                card_view:srcCompat="@drawable/ic_more_vert_black_24dp" />

            <ImageButton
                android:id="@+id/playButton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="51dp"
                android:background="#AFCBFF"
                android:backgroundTint="#AFCBFF"
                android:tint="@android:color/white"
                card_view:srcCompat="@android:drawable/ic_media_play" />

            <TextView
                android:id="@+id/title_text2"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/textView13"
                android:gravity="center"
                android:text="TextView"
                android:textAppearance="@android:style/TextAppearance.Material.Medium"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="62dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/textView16"
                android:text="Prepare:"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="64dp"
                android:text="Cycle: "
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="96dp"
                android:text="Cool Down: "
                android:textColor="@android:color/white"
                android:textStyle="bold"
                />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="31dp"
                android:text="Workout: "
                android:textColor="@android:color/white"
                android:textStyle="bold"
                />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView16"
                android:layout_alignTop="@+id/textView14"
                android:text="Sets: "
                android:textColor="@android:color/white"
                android:textStyle="bold"
                />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView17"
                android:layout_alignTop="@+id/textView15"
                android:text="Rest: "
                android:textColor="@android:color/white"
                android:textStyle="bold"
                />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="123dp"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignTop="@+id/textView22"
                android:text="Set Rest: "
                android:textColor="@android:color/white"
                android:textStyle="bold"
                />

            <TextView
                android:id="@+id/textView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/textView17"
                android:layout_alignTop="@+id/textView22"
                android:text="Total Time: "
                android:textColor="@android:color/white"
                android:textStyle="bold"
                />

            <TextView
                android:id="@+id/prepare_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/title_text2"
                android:layout_alignTop="@+id/textView13"
                android:text="00"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/cycles_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView14"
                android:layout_toEndOf="@+id/textView14"
                android:text="00"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/cooldown_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView15"
                android:layout_marginTop="0dp"
                android:layout_toEndOf="@+id/textView15"
                android:text="00"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/setrest_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView22"
                android:layout_toEndOf="@+id/textView23"
                android:text="00"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/workout_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView13"
                android:layout_toEndOf="@+id/textView16"
                android:text="00"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/sets_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView14"
                android:layout_toEndOf="@+id/textView17"
                android:text="00"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rest_text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView15"
                android:layout_toEndOf="@+id/textView18"
                android:text="00"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/totalTime_Text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/textView22"
                android:layout_toEndOf="@+id/textView24"
                android:text="00"
                android:textColor="@android:color/white"
                android:textStyle="bold" />
        </RelativeLayout>


    </android.support.v7.widget.CardView>

</LinearLayout>